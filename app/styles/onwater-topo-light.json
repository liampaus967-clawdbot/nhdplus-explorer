      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#ffffff",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 1, 16, 3],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 6, 0, 8, 0.5, 12, 1]
      }
    },
    {
      "id": "road-primary-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 6,
      "filter": ["==", ["get", "class"], "primary"],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#D9CAA6",
        "line-width": ["interpolate", ["linear"], ["zoom"], 6, 2, 16, 5],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 6, 0, 7, 0.4, 10, 0.7]
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 6,
      "filter": ["==", ["get", "class"], "primary"],
      "layout": { "line-cap": "round", "line-join": "round" },
