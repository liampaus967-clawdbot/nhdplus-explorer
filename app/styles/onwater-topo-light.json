{
  "version": 8,
  "name": "onWater Topo Light",
  "metadata": {
    "description": "onWater topo basemap with contours in feet, public lands, and terrain"
  },
  "sources": {
    "composite": {
      "url": "mapbox://mapbox.mapbox-streets-v8,mapbox.mapbox-terrain-v2",
      "type": "vector"
    },
    "mapbox-dem": {
      "type": "raster-dem",
      "url": "mapbox://mapbox.mapbox-terrain-dem-v1",
      "tileSize": 512,
      "maxzoom": 14
    },
    "mapbox-dem-hillshade": {
      "type": "raster-dem",
      "url": "mapbox://mapbox.mapbox-terrain-dem-v1",
      "tileSize": 512,
      "maxzoom": 14
    }
  },
  "terrain": {
    "source": "mapbox-dem",
    "exaggeration": 1.5
  },
  "satellite": {
    "satellite": {"paint": {"raster-opacity": 0.14}},
    "background": {
        "paint": {"background-color": "rgba(4, 8, 16, 0)"}
    }
},
  "sprite": "mapbox://sprites/mapbox/outdoors-v12",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#e8e4dc"
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "grass"],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#d8e0d0", 10, "#f5f5f2", 12, "#ffffff"],
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "wood"],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#c8d4bc", 10, "#f0f0ec", 12, "#ffffff"],
        "fill-opacity": 0.6
      }
    },
    {
      "id": "landcover-scrub",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "scrub"],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#d5ddc8", 10, "#f2f2ee", 12, "#ffffff"],
        "fill-opacity": 0.2
      }
    },
    {
      "id": "landcover-crop",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "crop"],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#FBFBFA", 10, "#f5f5f2", 12, "#ffffff"],
        "fill-opacity": 0.4
      }
    },
    {
      "id": "national-park-fill",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "filter": ["==", ["get", "class"], "national_park"],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#c5d4a8", 10, "#c5d4a8", 12, "#ecf2e1"],
        "fill-opacity": 0.45
      }
    },
    {
      "id": "national-park-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "filter": ["==", ["get", "class"], "national_park"],
      "paint": {
        "line-color": "#8fa068",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 0.2, 12, 1],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 8, 1, 12, 1, 14, 0]
      }
    },
    
    {
      "id": "national-park-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "minzoom": 6,
      "filter": [
        "all",
        ["==", ["get", "class"], "national_park"],
        ["any",
          ["all", ["==", ["get", "type"], "national_park"], [">=", ["zoom"], 3]],
          ["all", ["in", ["get", "type"], ["literal", ["national_forest", "national_monument"]]], [">=", ["zoom"], 8]],
          ["all", ["in", ["get", "type"], ["literal", ["state_forest", "state_park", "wilderness", "nature_reserve"]]], [">=", ["zoom"], 10]]
        ]
      ],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Bold", "Arial Unicode MS Bold"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 8, 10, 10, 12, 14, 14],
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1,
        "text-max-width": 8,
        "symbol-placement": "point"
      },
      "paint": {
        "text-color": "#6b8c44",
        "text-halo-color": "rgba(255, 255, 255, 0.9)",
        "text-halo-width": 2,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 8, 0, 9, 1]
      }
    },
    {
      "id": "park-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "poi_label",
      "minzoom": 6,
      "filter": [
        "all",
        ["==", ["get", "class"], "park_like"],
        ["<=", ["get", "filterrank"], ["+", ["step", ["zoom"], 0, 16, 1, 17, 2], ["match", ["get", "class"], "park_like", 1, 0]]]
      ],
      "layout": {
        "text-field": ["coalesce", ["get", "name_en"], ["get", "name"]],
        "text-font": ["DIN Pro Bold", "Arial Unicode MS Bold"],
        "text-size": [
          "step", ["zoom"],
          ["step", ["get", "sizerank"], 18, 5, 12],
          17,
          ["step", ["get", "sizerank"], 18, 13, 12]
        ],
        "text-max-width": 8,
        "symbol-placement": "point",
        "text-anchor": [
          "step", ["zoom"],
          ["step", ["get", "sizerank"], "center", 5, "top"],
          17,
          ["step", ["get", "sizerank"], "center", 13, "top"]
        ]
      },
      "paint": {
        "text-color": "#6b8c44",
        "text-halo-color": "rgba(255, 255, 255, 0.9)",
        "text-halo-width": 2,
        "text-opacity": 1
      }
    },
    {
      "id": "landuse-park-fill",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["match", ["get", "class"], ["park", "pitch", "grass"], true, false],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#d0dfc0", 10, "#f2f2ee", 12, "#ffffff"],
        "fill-opacity": 0.5
      }
    },
    {
      "id": "state-park-fill",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["all", ["==", ["get", "class"], "park"]],
      "paint": {
        "fill-color": ["interpolate", ["linear"], ["zoom"], 6, "#ccd8a5", 10, "#f0f0ec", 12, "#ffffff"],
        "fill-opacity": 0.55
      }
    },
    {
      "id": "state-park-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "park"],
      "paint": {
        "line-color": "#94a86c",
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 0.8, 14, 2],
        "line-opacity": 0.6
      }
    },
    {
      "id": "landcover-snow",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "snow"],
      "paint": {
        "fill-color": "#ffffff",
        "fill-opacity": 0.9
      }
    },
    {
      "id": "cemetery",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "cemetery"],
      "paint": {
        "fill-color": "#d8e0cc",
        "fill-opacity": 0.4
      }
    },
  
    {
      "id": "waterway-river",
      "type": "line",
      "source": "composite",
      "source-layer": "waterway",
      "filter": ["==", ["get", "class"], "river"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#a0d0e0",
        "line-width": ["interpolate", ["linear"], ["zoom"], 8, 1.5, 14, 4]
      }
    },
    {
      "id": "waterway-stream",
      "type": "line",
      "source": "composite",
      "source-layer": "waterway",
      "filter": ["match", ["get", "class"], ["stream", "canal"], true, false],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#a8d4e4",
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 0.5, 14, 2]
      }
    },
    {
      "id": "contour-line",
      "type": "line",
      "source": "composite",
      "source-layer": "contour",
      "filter": ["!=", ["%", ["get", "ele"], 80], 0],
      "paint": {
        "line-color": "#B37F33",
        "line-width": ["interpolate", ["linear"], ["zoom"], 11, 0.6, 14, 0.8, 16, 1],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 11, 0, 12, 0.5, 14, 0.6, 16, 0.7]
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "fill-color": "#b8dce8"
      }
    },
    {
      "id": "water-outline",
      "type": "line",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "line-color": "#0391E3",
        "line-width": ["interpolate", ["linear"], ["zoom"], 6, 0, 8, 0.5, 14, 1],
        "line-opacity": 0.8
      }
    },
    {
      "id": "hillshade",
      "type": "hillshade",
      "source": "mapbox-dem-hillshade",
      "paint": {
        "hillshade-illumination-direction": 315,
        "hillshade-illumination-anchor": "viewport",
        "hillshade-exaggeration": 0.9,
        "hillshade-shadow-color": "#2a3a2a",
        "hillshade-highlight-color": "#ffffff",
        "hillshade-accent-color": "#3a4a3a"
      }
    },
    {
      "id": "contour-line-index",
      "type": "line",
      "source": "composite",
      "source-layer": "contour",
      "filter": ["==", ["%", ["get", "ele"], 80], 0],
      "paint": {
        "line-color": "#B37F33",
        "line-width": ["interpolate", ["linear"], ["zoom"], 11, 1, 14, 1.3, 16, 1.6],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 11, 0, 12, 0.6, 14, 0.7, 16, 0.8]
      }
    },
    {
      "id": "contour-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "contour",
      "filter": ["==", ["%", ["get", "ele"], 80], 0],
      "layout": {
        "text-field": ["concat", ["to-string", ["round", ["*", ["get", "ele"], 3.28084]]], "ft"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 13, 9, 16, 11],
        "symbol-placement": "line",
        "text-max-angle": 25,
        "text-padding": 5
      },
      "paint": {
        "text-color": "#7a6855",
        "text-halo-color": "rgba(232, 228, 220, 0.8)",
        "text-halo-width": 1.5,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 13, 0, 14, 0.8]
      }
    },
    {
      "id": "aeroway-polygon",
      "type": "fill",
      "source": "composite",
      "source-layer": "aeroway",
      "filter": ["==", ["geometry-type"], "Polygon"],
      "paint": {
        "fill-color": "#e0dcd4"
      }
    },
    {
      "id": "aeroway-runway",
      "type": "line",
      "source": "composite",
      "source-layer": "aeroway",
      "filter": ["==", ["get", "type"], "runway"],
      "paint": {
        "line-color": "#d8d4cc",
        "line-width": ["interpolate", ["linear"], ["zoom"], 10, 1, 14, 6]
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "#dcd8d0",
        "fill-opacity": ["interpolate", ["linear"], ["zoom"], 14, 0, 15, 0.6]
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 6,
      "filter": ["match", ["get", "class"], ["path", "track"], true, false],
      "layout": {
        "line-cap": "round"
      },
      "paint": {
        "line-color": "#c4bca8",
        "line-width": ["interpolate", ["linear"], ["zoom"], 12, 0.8, 16, 2],
        "line-dasharray": [2, 1.5],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 6, 0, 8, 0.3, 12, 0.5, 14, 0.7]
      }
    },
    {
      "id": "road-minor-case",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 6,
      "filter": ["match", ["get", "class"], ["street", "street_limited", "service"], true, false],
      "layout": { "line-cap": "round", "line-join": "round" },
      "paint": {
        "line-color": "#d4d0c4",
        "line-width": ["interpolate", ["linear"], ["zoom"], 12, 2, 16, 5],
        "line-opacity": ["interpolate", ["linear"], ["zoom"], 6, 0, 8, 0.3, 12, 0.4, 14, 0.6]
      }
    },
    {
      "id": "road-minor",
      "type": "line",
